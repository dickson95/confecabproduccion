<%
@seguimiento = Seguimiento.new
@user = {}
@users = User.select("id, name").each{ |e|  @user[e.id] = e.name}
%>
$("table > tbody > tr").last().prev().replaceWith("<%= j render 'control_lotes/row', control_lote: @control%>");
btn_cambio = $("a[data-action='cambio_estado']");
button = "<%= j render 'control_lotes/button_cambio_estado' %>";
if(btn_cambio.length > 0) {
    btn_cambio.replaceWith(button);
}else{
    $("table").closest(".box").next().children().append(button);
}
