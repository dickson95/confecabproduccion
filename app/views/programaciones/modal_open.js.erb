<% # Sección para brir modal %>
<%if @lotes.nil?%>
$("#modal_add").html("<%=j render partial: "new"%>");
$("#modal_add").modal("toggle");




<%elsif @lotes.empty?%>
<% # Sección para cerrar modal %>
$("#modal_add").modal("toggle");





<%else%>
<% # Añadir los nuevos lotes a la tabla y cerrar modal %>
<%date = Programacion.date_split params[:month]%>
<%month = date[:month]%>
<%month = @meses.fetch(month.to_i - 1)%>
$("#<%=month%>t").find("tbody").append("<%=j render partial: "last_rows_table", locals:{mes: month}%>");
$("#row_generate").remove();
$("#modal_add").modal("toggle");
<%end%>

