<%date = Programacion.date_split params[:month]%>
<%month = date[:month]%>
<%month = @meses.fetch(month.to_i - 1)%>
$("#<%=params[:month]%>").addClass("active");
$("div.tab-content").find(".tab-content").find(".active").removeClass("in active");
$("#<%=month%>").children().remove();
$("#<%=month%>").append("<%= j render "table", mes: month%>");
$("#<%=month%>").addClass("in active");
